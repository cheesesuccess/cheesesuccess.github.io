    "@typescript-eslint/eslint-plugin": "^5.65.0",
    "@typescript-eslint/parser": "^5.65.0",
    "@vanilla-extract/vite-plugin": "^3.2.0",
    "css-class-generator": "^2.0.0",
    "css-tree": "^2.3.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^6.4.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5",
    "vite": "^4.4.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-solid": "^2.4.0"
  },
