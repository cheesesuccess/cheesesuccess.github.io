    "@types/offscreencanvas": "^2019.6.4",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@vanilla-extract/css": "^1.7.0",
    "@vanilla-extract/dynamic": "^2.0.2",
    "@vanilla-extract/sprinkles": "^1.4.0",
    "firebase": "^9.23.0",
    "buffer": "^6.0.3",
    "fuzzysort": "^3.1.0",
    "idb-keyval": "^6.1.0",
